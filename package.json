{
  "name": "corecdtl",
  "version": "0.1.4",
  "description": "High-performance customizable HTTP engine",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gypfile": true,
  "keywords": [
    "http",
    "server",
    "parser",
    "engine",
    "native",
    "addon",
    "low-level"
  ],
  "scripts": {
    "build": "node-gyp rebuild && rm -rf dist && tsc",
    "rebuild": "node-gyp rebuild --clean && rm -rf dist && tsc",
    "test": "vitest run",
    "benchmark": "node benchmark/e2e/run.js",
    "prepublishOnly": "npm run build",
    "example": "node example/server.js",
    "example:web": "node example/server.web.js"
  },
  "files": [
    "dist",
    "native",
    "binding.gyp",
    "CMakeLists.txt",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0"
  },
  "devDependencies": {
    "@fastify/express": "^4.0.4",
    "@types/bindings": "^1.5.5",
    "@types/node": "^24.8.1",
    "autocannon": "^8.0.0",
    "cmake-js": "^7.3.1",
    "express": "^5.2.1",
    "fastify": "^5.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DirikTi/corecdtl.git"
  },
  "bugs": {
    "url": "https://github.com/DirikTi/corecdtl/issues"
  },
  "homepage": "https://github.com/DirikTi/corecdtl#readme"
}
